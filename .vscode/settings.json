{
  "emeraldwalk.runonsave": {
    "commands": [{
      "cmd": "node ${workspaceFolder}/.vscode/.runonsave.js ${file}",
      "match": ".(js|json|ts)$"
    }]
  },
  "vscode-neovim.ctrlKeysForInsertMode": [
    "d",
    "h",
    "m",
    "r",
    "t",
    "u",
  ],
  "vscode-neovim.ctrlKeysForNormalMode": [
    "b",
    "d",
    "e",
    "h",
    "i",
    "k",
    "l",
    "m",
    "r",
    "t",
    "u",
    "/",
    "]",
    "right",
    "left",
    "up",
    "down",
    "backspace",
    "delete"
  ]
}
